import time

#start timer
start_time = time.time()

#Benchmark 1 operations
result = 0.0
for i in range (10.0**10.0):
    result += i
for i in range (5.0*10.0**9.0):
    result *= i
for i in range ((2.0*10.0**9.0)+1):
    result /= i+1

#stop timer
stop_time = time.time()

execution_time = stop_time - start_time
print("64 bit Floating point operation benchmark execution time:", execution_time)