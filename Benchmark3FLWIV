import time

num_elements = 5 * (10**9)
element_size = 4 #bytes
array = []

#writes array
start = time.perf_counter()
for i in range(num_elements):
    array.append(i)
stop = time.perf_counter
write_array  = stop-start

#reads from the array
start = time.perf_counter
for i in range (len(array)):
        value = array [i]
stop = time.perf_counter()

